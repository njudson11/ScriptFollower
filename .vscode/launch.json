{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/script-follower-vite/src",
      "sourceMaps": true,
      "preLaunchTask": "npm: run dev"
    },
    {
      "name": "Attach Chrome",
      "type": "chrome",
      "request": "attach",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/script-follower-vite/src",
      "sourceMaps": true
    }
  ]
}